[color]
	ui = auto
	diff = auto
	status = auto
	log = auto
[alias]
	st = status -s
	co = checkout
	br = branch
	last = diff HEAD^ HEAD
	cp = cherry-pick
	l = log --no-merges --graph --abbrev-commit --decorate --date=short --pretty='%C(bold green)%h%C(reset) %C(bold cyan)%ad%C(reset) %s %C(yellow)%aN%C(reset)'
	lg = log --no-merges --graph --abbrev-commit --decorate --date=short --pretty='%C(bold green)%h%C(reset) %C(bold cyan)%ad%C(reset) %s %C(yellow)%aN%C(reset)'
	lg1 = log --no-merges --graph --abbrev-commit --decorate --date=short --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
	lg2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
[user]
	name = qinghao
	email = qingxianhao@gmail.com
[color "diff"]
	whitespace = red reverse
[http]
	postBuffer = 524288000
[core]
	excludesfile = /home/qinghao/.gitignore
